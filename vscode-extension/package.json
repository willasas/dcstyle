{
  "name": "dcstyle-intellisense",
  "displayName": "DC Style Intellisense",
  "description": "Intelligent autocompletion for DC Style utility classes",
  "version": "1.0.0",
  "publisher": "dcstyle",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "html",
        "extensions": [".html"]
      },
      {
        "id": "css",
        "extensions": [".css"]
      },
      {
        "id": "scss",
        "extensions": [".scss"]
      },
      {
        "id": "vue",
        "extensions": [".vue"]
      },
      {
        "id": "react",
        "extensions": [".jsx", ".tsx"]
      }
    ],
    "completionItems": [
      {
        "language": "html",
        "path": "./src/completions.json"
      },
      {
        "language": "css",
        "path": "./src/completions.json"
      },
      {
        "language": "scss",
        "path": "./src/completions.json"
      },
      {
        "language": "vue",
        "path": "./src/completions.json"
      },
      {
        "language": "react",
        "path": "./src/completions.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:vue",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0",
    "mocha": "^10.0.0",
    "@types/mocha": "^10.0.0"
  }
}
